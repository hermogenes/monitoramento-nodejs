{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./types"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
  }
}
